{
  "name": "material-draft",
  "version": "0.0.1-beta1",
  "description": "Rich Text Editor using Draft.js and Material-UI",
  "main": "index.js",
  "scripts": {
    "prestart": "npm install",
    "start": "webpack-dev-server --config ./tooling/webpack.config.js --port 8081 --inline --quiet",
    "lint": "eslint ./src",
    "clean": "rm -rf ./dist/public",
    "prebuild": "npm run clean",
    "build": "webpack --config ./tooling/webpack.config.js --progress --colors --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simpixelated/material-draft.git"
  },
  "keywords": [
    "react",
    "draft-js",
    "material-ui",
    "rich text editor"
  ],
  "author": "Jordan \"Simpixelated\" Kohl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simpixelated/material-draft/issues"
  },
  "homepage": "https://github.com/simpixelated/material-draft#readme",
  "dependencies": {
    "draft-js": "0.1.0",
    "lodash": "4.0.1",
    "material-ui": "0.15.0-alpha.1",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-tap-event-plugin": "0.2.2"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.3.17",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.3",
    "babel-plugin-transform-class-properties": "6.4.0",
    "babel-polyfill": "6.6.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "css-loader": "0.23.1",
    "eslint": "2.2.0",
    "eslint-plugin-lodash": "1.2.1",
    "eslint-plugin-react": "4.1.0",
    "html-webpack-plugin": "2.9.0",
    "style-loader": "0.13.0",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  }
}
